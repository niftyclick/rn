{
  "expo": {
    "name": "NiftyClick",
    "slug": "nifty-click",
    "owner": "ritvij14",
    "privacy": "hidden",
    "version": "1.0.0",
    "scheme": "niftyclick",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/niftyclick.png",
      "backgroundColor": "#11141E"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon_small.png"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
